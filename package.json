{
  "name": "rainbowui-ajax",
  "version": "2.0.2",
  "description": "this is a rainbowui-react-project.",
  "main": "",
  "scripts": {
    "dev": "node ./cfg/server.js --env=dev --port=8000",
    "build": "npm run clean &  npm run build:dev & npm run build:release",
    "build:dev": "webpack --env=dev",
    "build:release": "webpack --env=dist",
    "lint": "eslint ./src",
    "update": "rimraf cfg/* node_modules/* .babelrc .eslintrc webpack.config.js & npm i generator-rainbowui-cli -g & yo rainbowui-cli react-update",
    "clean": "rimraf dist/*",
    "init": "rimraf src/components/* src/images/* src/pages/* src/styles/* & copyfiles -f ./cfg/template/* ./src/",
    "doc": "rimraf ../../rainbow-other/developer/src/doc/ajax/doc/* & jsdoc src -r -c conf.json -d ../../rainbow-other/developer/src/doc/ajax/doc & copyfiles -u 8 ../../rainbow-other/developer/src/doc/ajax/template/*.* ../../rainbow-other/developer/src/doc/ajax/doc"
  },
  "keywords": [],
  "author": "Rainbow Team",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "bower-webpack-plugin": "^0.1.9",
    "copyfiles": "^1.0.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.23.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "html-webpack-template": "^6.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "minimist": "^1.2.0",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "react": "^15.4.2",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "html2canvas": "^1.0.0-alpha.9",
    "jquery-slim": "^3.0.0",
    "url-search-params": "^0.6.1",
    "whatwg-fetch": "^2.0.2"
  }
